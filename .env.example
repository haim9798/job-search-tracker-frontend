# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_VERSION=v1

# Application Configuration
REACT_APP_APP_NAME=Job Search Tracker
REACT_APP_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_DEVTOOLS=true

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# Build Configuration
GENERATE_SOURCEMAP=true
ANALYZE=false

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1

# Error Reporting
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ERROR_REPORTING_URL=

# Analytics (if using external analytics)
REACT_APP_ANALYTICS_ID=
REACT_APP_ANALYTICS_PROVIDER=

# Feature Toggles
REACT_APP_ENABLE_STATISTICS=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_IMPORT=true
REACT_APP_ENABLE_NOTIFICATIONS=true
